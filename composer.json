{
    "name": "strixnl/uxupgrades",
    "description": "Plugin for Shopware UX Upgrades",
    "version": "1.9.2",
    "type": "shopware-platform-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Joris"
        }
    ],
    "require": {
        "shopware/core": "~6.7.0"
    },
    "extra": {
        "shopware-plugin-class": "StrixNLUxUpgrades\\StrixNLUxUpgrades",
        "label": {
            "de-DE": "Strix UX Upgrades",
            "en-GB": "Strix UX Upgrades",
            "nl-NL": "Strix UX Upgrades"
        }
    },
    "autoload": {
